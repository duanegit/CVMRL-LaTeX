}

@article{KALASKA91,
title = "Reaching movements to visual targets: neuronal representations of sensori-motor transformations",
journal = "Seminars in Neuroscience",
volume = "3",
number = "1",
pages = "67 - 80",
year = "1991",
issn = "1044-5765",
author = "Kalaska, J.",
abstract = "Reaching movements can be described in many different coordinate frameworks, from the path of the hand through space, to the patterns of causative muscle contractions. It is logical, therefore, to propose that the brain contains representations of reaching movements in each of these reference frames, and controls movement by making serial transformations of movement-related signals among the coordinate frameworks. The transformation from the desired kinematic parameters of movement (direction, velocity, amplitude, etc) to the requisite parameters of movement dynamics (forces, torques, muscle activity) is a particularly complex problem in the skeletomotor system. Although our understanding of motor control processes is advancing rapidly, there is still surprisingly little conclusive evidence concerning the neuronal mechanisms of this serial hierarchical model for the planning of reaching movements. Parallel-processing mechanisms are also likely to be very important."
}

