@ARTICLE{VEDALD07,
TITLE={Moving Forward in Structure From Motion},
AUTHOR={Andrea Vedaldi and Gregorio Guidi and Stefano Soatto},
JOURNAL={Computer Vision and Pattern Recognition, 2007. CVPR '07. IEEE Conference on},
year={17-22 June 2007},
pages={1-7},
abstract={It is well-known that forward motion induces a large number of local minima in the instantaneous least-squares reprojection error. This is caused in part by singularities in the error landscape around the forward direction, and presents a challenge in using existing algorithms for structure-from-motion in autonomous navigation applications. In this paper we prove that imposing a bound on the reconstructed depth of the scene makes the least-squares reprojection error continuous. This has implications for autonomous navigation, as it suggests simple modifications for existing algorithms to minimize the effects of local minima in forward translation.},
doi={10.1109/CVPR.2007.383117},
}

